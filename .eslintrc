{
  "rules": {
    "indent": [
      2,
      4
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "node": true,
    "browser": true,
    "jasmine": true,
    "protractor": true,
    "jquery": true
  },
  "globals": {
    "inject": false
  },
  "extends": [
    "eslint:recommended",
    "angular"
  ]
}
